          SQLTask:IsStoredProc="True"
          SQLTask:SqlStatementSource="SET NOCOUNT ON&#xA;&#xA;TRUNCATE TABLE D_Tiempo&#xA;&#xA;DECLARE @CurrentDate DATE = '2019-01-01'&#xA;DECLARE @EndDate DATE = '2019-12-31'&#xA;&#xA;WHILE @CurrentDate &lt; @EndDate&#xA;BEGIN&#xA;   INSERT INTO D_Tiempo (&#xA;   PK_Tiempo,&#xA;   Fecha,&#xA;   dia,&#xA;   dia_semana,&#xA;   semana,&#xA;   mes,&#xA;   cuatrimestre,&#xA;   ano&#xA;   )&#xA;   SELECT&#xA;   PK_Tiempo = YEAR(@CurrentDate) * 10000 + MONTH(@CurrentDate) * 100 + DAY(@CurrentDate),&#xA;      Fecha = @CurrentDate,&#xA;      dia = DAY(@CurrentDate),&#xA;      dia_semana = DATEPART(dw, @CurrentDate),&#xA;      dia_semana = DATEPART(wk, @CurrentDate),&#xA;      mes = MONTH(@CurrentDate),&#xA;      cuatrimestre = DATEPART(q, @CurrentDate),&#xA;      ano = YEAR(@CurrentDate)&#xA;   SET @CurrentDate = DATEADD(DD, 1, @CurrentDate)&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
